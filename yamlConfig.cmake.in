# prevent multiple inclusion
if(_yaml_CONFIG_INCLUDED)
  return()
endif()
set(_yaml_CONFIG_INCLUDED TRUE)

# output package information
if(NOT yaml_FIND_QUIETLY)
  message(STATUS "Found yaml: @PACKAGE_VERSION@ (${yaml_DIR})")
endif()

# include all config extra files
set(_extras "ament_cmake_export_libraries-extras.cmake")
foreach(_extra ${_extras})
  include("${libyaml_vendor_DIR}/${_extra}")
endforeach()

set(yaml_LIBRARIES ${libyaml_vendor_LIBRARIES})
